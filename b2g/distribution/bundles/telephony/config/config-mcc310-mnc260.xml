<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- If the MCC/MNC does not match any known configs, LTE will be used and
         default behavior is to assume GSM for RAT -->
    <config_volte_replacement_rat>3</config_volte_replacement_rat>

    <!-- Flag indicating whether the IMS service can be turned off. If false
         then the service will not be turned-off completely, but individual
         Features can be disabled -->
    <config_ims_service_allow_turnoff>false</config_ims_service_allow_turnoff>

    <!-- Flag indicating whether to display WIFI calling Profile or not -->
    <config_display_wifi_calling_profile>true</config_display_wifi_calling_profile>

    <!-- This indicates default wifi calling preference.
         Refer to WifiCallingPreference in ims/imsIF.proto -->
    <config_default_wifi_calling_preference>1</config_default_wifi_calling_preference>

    <!-- Flag indicating if tty is enabled -->
    <config_tty_enabled>true</config_tty_enabled>

    <!-- Flag specifying whether WFC over IMS is available on device -->
    <config_device_wfc_ims_available>true</config_device_wfc_ims_available>

    <config_registration_error_codes_list>
