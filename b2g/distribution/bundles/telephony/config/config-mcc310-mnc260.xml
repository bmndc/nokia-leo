    <!-- Flag indicating whether to use Ims always for emergency call -->
    <config_use_ims_always_for_emergency_call>true</config_use_ims_always_for_emergency_call>

    <!-- Do not translate. Defines the slots is
         Two Digit Number for dialing normally not USSD -->
    <config_two_digit_number_pattern>
    </config_two_digit_number_pattern>

    <!-- Flag indicating whether strict threshold is used, or lenient threshold is used,
     when evaluating RSRP for LTE antenna bar display
     0. Strict threshold
     1. Lenient threshold -->
    <config_LTE_RSRP_threshold_type>1</config_LTE_RSRP_threshold_type>

    <!--Flag indicating if host is to be shown in list in IMS conference call-->
    <config_filter_ims_host_particpant>true</config_filter_ims_host_particpant>
</resources>
