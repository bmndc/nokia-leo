<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- If the MCC/MNC does not match any known configs, LTE will be used and
         default behavior is to assume GSM for RAT -->
    <config_volte_replacement_rat>6</config_volte_replacement_rat>

    <!-- Flag indicating whether the IMS service can be turned off. If false
         then the service will not be turned-off completely, but individual
         Features can be disabled -->
    <config_ims_service_allow_turnoff>false</config_ims_service_allow_turnoff>

    <!-- Flag indicating whether to display WIFI calling Profile or not -->
    <config_display_wifi_calling_profile>false</config_display_wifi_calling_profile>

    <!-- This indicates default wifi calling preference.
         Refer to WifiCallingPreference in ims/imsIF.proto -->
    <config_default_wifi_calling_preference>0</config_default_wifi_calling_preference>

    <!-- Flag indicating if tty is enabled -->
    <config_tty_enabled>true</config_tty_enabled>

    <!-- Flag specifying whether WFC over IMS is available on device -->
    <config_device_wfc_ims_available>true</config_device_wfc_ims_available>

    <!-- List based on revision 00006, last modified 8/18/2014 -->
    <config_eri_list VersionNumber="00006"
             NumberOfEriEntries="7"
             EriFileType="2">

             <EriInfo RoamingIndicator="64"
                      IconIndex="1"
                      IconMode="0"
                      EriText="Verizon Wireless"
                      CallPromptId="0"
                      AlertId="0"/>

             <EriInfo RoamingIndicator="65"
                      IconIndex="1"
                      IconMode="0"
                      EriText="Verizon Wireless"
                      CallPromptId="0"
                      AlertId="0"/>

             <EriInfo RoamingIndicator="66"
                      IconIndex="1"
                      IconMode="0"
                      EriText="Extended Network"
                      CallPromptId="0"
                      AlertId="4"/>

             <EriInfo RoamingIndicator="67"
                      IconIndex="0"
                      IconMode="0"
                      EriText="Extended Network"
                      CallPromptId="0"
                      AlertId="4"/>

             <EriInfo RoamingIndicator="68"
                      IconIndex="0"
                      IconMode="0"
                      EriText="Roaming"
                      CallPromptId="0"
                      AlertId="5"/>

             <EriInfo RoamingIndicator="69"
                      IconIndex="2"
                      IconMode="0"
                      EriText="Extended Network"
                      CallPromptId="0"
                      AlertId="4"/>

             <EriInfo RoamingIndicator="70"
                      IconIndex="2"
                      IconMode="0"
                      EriText="Roaming"
                      CallPromptId="0"
                      AlertId="5"/>

    </config_eri_list>

    <!-- Flag indicating whether to use Ims always for emergency call -->
    <config_use_ims_always_for_emergency_call>true</config_use_ims_always_for_emergency_call>

    <!-- Do not translate. Defines the slots is
         Two Digit Number for dialing normally not USSD -->
    <config_two_digit_number_pattern>
    </config_two_digit_number_pattern>

    <!-- Flag indicating whether strict threshold is used, or lenient threshold is used,
     when evaluating RSRP for LTE antenna bar display
     0. Strict threshold
     1. Lenient threshold -->
    <config_LTE_RSRP_threshold_type>1</config_LTE_RSRP_threshold_type>

    <!--Flag indicating if host is to be shown in list in IMS conference call-->
    <config_filter_ims_host_particpant>true</config_filter_ims_host_particpant>

</resources>
